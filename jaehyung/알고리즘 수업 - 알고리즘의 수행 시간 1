import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] nArr = new int[n];
		
		for(int i = 0; i < nArr.length; i++) {
			nArr[i] = sc.nextInt();
		}
		
		int i = n / 2;
		int result = nArr[i];
		
		System.out.println(result);
		// 뭘 하라는 거지 ?~?
 	}
}
